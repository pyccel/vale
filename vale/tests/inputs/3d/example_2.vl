Domain(dim=3,kind="structured") :: Omega
Space(domain=Omega,kind='h1')   :: V
Field(space=V)                  :: phi 
Field(space=V)                  :: q 
Function(x,y,z)                 :: f 

b(v::V) := < f * v + q * v >_Omega 
a(v::V, u::V) := < dx(v) * dx(u) + dy(v) * dy(u) + dz(v) * dz(u) + q * v >_Omega
