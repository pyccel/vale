Domain(dim=3,kind='structured') :: Omega
Space(domain=Omega,kind='h1')   :: V
Field(space=V)                  :: phi
Field(space=V)                  :: psi
Function(x)                     :: f
Function(x)                     :: g

b1(u::V) := < f * u >_Omega
b2(w::V) := < g * w >_Omega

a11(v::V, u::V) := < dx(v) * dx(u) + dy(v) * dy(u) + dz(v) * dz(u) >_Omega
a22(v::V, u::V) := < dx(v) * dx(u) + dy(v) * dy(u) + dz(v) * dz(u) >_Omega

b((v1,v2)::V) := b1(v1) + b2(v2)
a((v1,v2)::V,(u1,u2)::V) := a11(v1,u1) + a22(v2,u2)
